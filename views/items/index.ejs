<h2>Search items</h2>
<form action="/items" method="GET">
    <label>Name</label>
    <input type="text" name="name" value="<%= searchItems.name %>">
    <button type="submit">Search</button>
</form>
<ol class="centered-list">
    
        <% items.forEach(item=> { %>
<li class="item-list">
            <%= item.name %>
    
    <% }) %></li>
</ol>